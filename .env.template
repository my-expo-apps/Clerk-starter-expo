# Copy this file to one of:
# - .env.dev
# - .env.staging
# - .env.prod
#
# Never commit real secrets.

# Expo (client-safe)
EXPO_PUBLIC_ENV=dev
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...

# Supabase (client-safe anon key + URL)
SUPABASE_URL=https://YOUR-PROJECT-REF.supabase.co
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Clerk federation (public metadata)
CLERK_JWT_ISSUER=https://YOUR-CLERK-ISSUER/
CLERK_EXPECTED_AUDIENCE=YOUR_EXPECTED_AUDIENCE

# CLI provisioning (secret)
SUPABASE_PROJECT_REF=YOUR_PROJECT_REF
SUPABASE_DB_PASSWORD=YOUR_DB_PASSWORD

# Health-check / tests (secret)
CLERK_TEST_JWT=YOUR_TEST_JWT